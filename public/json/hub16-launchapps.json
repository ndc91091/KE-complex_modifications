{
  "title": "Hub16 Apps",
  "rules": [
    {
      "description": "Launch apps with Hub16 macropad (template)",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "device_if",
              "identifiers": [
                {
                  "product_id": 36723,
                  "vendor_id": 65261
                }
              ]
            }
          ],
          "type": "basic",
