{
  "title": "Workaround broken s,w,2,tab,esc,< by fn+neighbour on Apple internal kbd only",
  "rules": [
    {
      "description": "Change fn + d to s",
      "manipulators": [
        {
         "conditions": [
            {
              "type": "device_if",
              "identifiers": [
                {
                  "vendor_id": 1452,
                  "product_id": 589
                }
              ]
            }
          ],
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "s"
            }
          ]
        }
      ]
    },
