{
    "title": "Command_L|Command_R + Return to Backslash",
    "rules": [
        {
            "description": "Command_[L|R] (⌘) + Return (⏎) to Backslash and Command_[L|R] (⌘) + Shift (⇧) + Return (⏎) to Pipe (|)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("return_or_enter", ["command"], ["left_shift", "right_shift"]) %>,
                    "to": <%= to([["backslash"]]) %>
                }
            ]
        }
    ]
}
